#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/20015058/Documents/Eliseo/Invitation-wedding-pj/serverless-invitation-wedding/packages/newsLetter/PostEmail/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/20015058/Documents/Eliseo/Invitation-wedding-pj/serverless-invitation-wedding/packages/newsLetter/PostEmail/node_modules/.pnpm/node_modules"
fi
if [ -x "$basedir/bash" ]; then
  exec "$basedir/bash"  "$basedir/../../bin/nenv" "$@"
else
  exec bash  "$basedir/../../bin/nenv" "$@"
fi
